{
  "name": "waltodo",
  "version": "1.0.0",
  "description": "A CLI todo application using Sui blockchain and Walrus storage",
  "main": "dist/src/index.js",
  "type": "commonjs",
  "bin": "bin/waltodo-direct",
  "scripts": {
    "prestart": "chmod +x ./bin/run.js ./bin/waltodo ./bin/waltodo-direct",
    "build": "tsc",
    "start": "node bin/run.js",
    "dev": "ts-node src/index.ts",
    "clean": "rm -rf dist",
    "prepare": "npm run clean && npm run build && npm run manifest",
    "link": "npm link",
    "global-install": "./install-global.sh",
    "test": "jest",
    "postinstall": "chmod +x ./bin/run.js ./bin/waltodo ./bin/waltodo-direct && touch oclif.manifest.json",
    "upload-image": "ts-node scripts/upload-default-image.ts",
    "manifest": "touch oclif.manifest.json",
    "postbuild": "npm run manifest",
    "fix-cli": "./fix-cli.sh",
    "local-install": "mkdir -p ~/.local/bin && npm pack && tar -xzf waltodo-1.0.0.tgz && cp -r package/bin/* ~/.local/bin/ && chmod +x ~/.local/bin/waltodo && rm -rf package waltodo-1.0.0.tgz"
  },
  "keywords": [
    "cli",
    "todo",
    "sui",
    "blockchain",
    "walrus"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@inquirer/prompts": "^3.3.0",
    "@mysten/sui": "^1.29.1",
    "@mysten/wallet-adapter-base": "^0.9.0",
    "@mysten/wallet-standard": "^0.14.9",
    "@mysten/walrus": "^0.0.21",
    "@oclif/core": "^3.25.2",
    "@smithery/cli": "^1.1.84",
    "axios": "^1.9.0",
    "chalk": "^4.1.2",
    "dotenv": "^16.4.5",
    "form-data": "^4.0.0",
    "inquirer": "^9.2.15",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@oclif/test": "^3.2.4",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/node-fetch": "^2.6.11",
    "@types/web": "^0.0.142",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "bin": "waltodo",
    "dirname": "waltodo",
    "commands": "./dist/src/commands",
    "plugins": [],
    "topicSeparator": ":",
    "macos": {
      "identifier": "com.waltodo.cli"
    },
    "topics": {
      "simple": {
        "description": "Simple todo management commands"
      }
    }
  },
  "engines": {
    "node": ">=23.11.0",
    "npm": ">=10.9.2"
  }
}
