{
  "name": "waltodo",
  "version": "1.0.0",
  "description": "A CLI todo application using Sui blockchain and Walrus storage",
  "main": "dist/index.js",
  "bin": {
    "waltodo": "dist/index.js"
  },
  "scripts": {
    "prestart": "chmod +x ./bin/run.js",
    "build": "tsc",
    "start": "node bin/run.js",
    "dev": "ts-node src/index.ts",
    "clean": "rm -rf dist",
    "prepare": "npm run clean && npm run build",
    "link": "npm link",
    "test": "jest",
    "postinstall": "chmod +x ./bin/run.js",
    "upload-image": "ts-node scripts/upload-default-image.ts"
  },
  "keywords": [
    "cli",
    "todo",
    "sui",
    "blockchain",
    "walrus"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@inquirer/prompts": "^3.0.0",
    "@mysten/sui": "^1.29.1",
    "@mysten/sui.js": "^0.54.1",
    "@mysten/wallet-adapter-base": "^0.9.0",
    "@mysten/walrus": "^0.0.21",
    "@oclif/core": "^2.11.7",
    "chalk": "^4.1.2",
    "dotenv": "^16.3.1",
    "form-data": "^4.0.0",
    "inquirer": "^8.2.5",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.4",
    "@types/web": "^0.0.228",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "bin": "waltodo",
    "dirname": "waltodo",
    "commands": "./dist/commands",
    "plugins": [],
    "topicSeparator": " "
  }
}
