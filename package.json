{
  "name": "waltodo",
  "version": "1.0.0",
  "description": "A CLI todo application using Sui blockchain and Walrus storage",
  "main": "dist/src/index.js",
  "type": "commonjs",
  "bin": "bin/waltodo-direct",
  "scripts": {
    "prestart": "chmod +x ./bin/run.js ./bin/waltodo ./bin/waltodo-direct",
    "build": "tsc",
    "start": "node bin/run.js",
    "dev": "ts-node src/index.ts",
    "clean": "rm -rf dist",
    "prepare": "pnpm run clean && pnpm run build && pnpm run manifest",
    "link": "pnpm link",
    "global-install": "./install-global.sh",
    "test": "jest --no-typecheck",
    "postinstall": "chmod +x ./bin/run.js ./bin/waltodo ./bin/waltodo-direct && touch oclif.manifest.json",
    "upload-image": "ts-node scripts/upload-default-image.ts",
    "manifest": "touch oclif.manifest.json",
    "postbuild": "pnpm run manifest",
    "fix-cli": "./fix-cli.sh",
    "local-install": "mkdir -p ~/.local/bin && pnpm pack && tar -xzf waltodo-1.0.0.tgz && cp -r package/bin/* ~/.local/bin/ && chmod +x ~/.local/bin/waltodo && rm -rf package waltodo-1.0.0.tgz"
  },
  "keywords": [
    "cli",
    "todo",
    "sui",
    "blockchain",
    "walrus"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@inquirer/prompts": "^3.3.0",
    "@mysten/sui": "^1.29.1",
    "@mysten/sui.js": "^0.54.1",
    "@mysten/walrus": "0.0.21",
    "@oclif/core": "^3.25.2",
    "@smithery/cli": "^1.1.84",
    "axios": "^1.9.0",
    "chalk": "^4.1.2",
    "dotenv": "^16.4.5",
    "form-data": "^4.0.0",
    "image-size": "^2.0.2",
    "inquirer": "^9.2.15",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-typescript": "^7.24.3",
    "@jest/globals": "^29.6.3",
    "@jest/test-sequencer": "^29.6.3",
    "@jest/types": "^29.6.3",
    "@oclif/test": "^3.2.15",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/node-fetch": "^2.6.11",
    "@types/sinon": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "babel-jest": "^29.6.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.6.3",
    "jest-environment-jsdom": "^29.6.3",
    "jest-environment-node": "^29.6.3",
    "jest-extended": "^4.0.2",
    "jest-mock": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "prettier": "^3.2.5",
    "sinon": "^17.0.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "5.0.4"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "bin": "waltodo",
    "dirname": "waltodo",
    "commands": "./dist/src/commands",
    "plugins": [],
    "topicSeparator": ":",
    "macos": {
      "identifier": "com.waltodo.cli"
    },
    "topics": {
      "account": {
        "description": "Manage Sui accounts"
      },
      "simple": {
        "description": "Simple todo management commands"
      }
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}