{
  "name": "@waltodo/config-loader",
  "version": "1.0.0",
  "description": "Runtime network configuration loader for WalTodo applications",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "waltodo",
    "config",
    "loader",
    "runtime",
    "network",
    "sui",
    "walrus"
  ],
  "author": "WalTodo Team",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.15.21",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "rollup": "^4.13.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=10.11.0"
  },
  "packageManager": "pnpm@10.11.0"
}