{
  "name": "@waltodo/sui-client",
  "version": "1.0.0",
  "description": "Unified Sui client package for WalTodo CLI and frontend with @mysten/dapp-kit wrapper",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "require": "./dist/react.js",
      "import": "./dist/react.mjs",
      "types": "./dist/react.d.ts"
    },
    "./vanilla": {
      "require": "./dist/vanilla.js",
      "import": "./dist/vanilla.mjs",
      "types": "./dist/vanilla.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json",
    "build:dev": "tsc --skipLibCheck",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "sui",
    "blockchain",
    "dapp-kit",
    "react",
    "client",
    "wallet"
  ],
  "author": "WalTodo",
  "license": "ISC",
  "dependencies": {
    "@mysten/sui": "^1.30.1",
    "@mysten/dapp-kit": "^0.14.32",
    "@tanstack/react-query": "^5.51.1",
    "@waltodo/config-loader": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "@types/react": "^18.3.3",
    "@types/jest": "^29.5.12",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/user-event": "^14.4.3",
    "jest": "^29.6.3",
    "jest-environment-jsdom": "^29.6.3",
    "typescript": "^5.8.3",
    "ts-jest": "^29.1.2"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}