    "encoding": "^0.1.13",
    "fast-equals": "^5.2.2",
    "framer-motion": "^12.15.0",
    "fuse.js": "^7.1.0",
    "goober": "^2.1.16",
    "idb": "^8.0.3",
    "internmap": "^2.0.3",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.511.0",
    "nanoid": "^5.1.5",
    "next": "14.2.18",
