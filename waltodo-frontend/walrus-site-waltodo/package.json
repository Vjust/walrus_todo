    "@types/react-dom": "18.3.1",
    "@types/react-window": "^1.8.8",
    "@types/react-window-infinite-loader": "^1.0.9",
    "assert": "^2.1.0",
    "autoprefixer": "10.4.20",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "chrome-launcher": "^1.1.0",
