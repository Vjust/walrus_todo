{
  "name": "@walrus-todo/walrus-site",
  "version": "0.1.0",
  "private": true,
  "description": "Static export version of WalTodo frontend for Walrus deployment",
  "scripts": {
    "setup-config": "node ../setup-config.js",
    "dev": "pnpm run setup-config && next dev",
    "build": "pnpm run setup-config && next build",
    "build:export": "pnpm run clean && pnpm run setup-config && next build",
    "build:clean": "pnpm run clean && pnpm run build:export",
    "build:dev": "./scripts/optimized-build.sh -e development",
    "build:staging": "./scripts/optimized-build.sh -e staging -c",
    "build:prod": "./scripts/optimized-build.sh -e production -c --jobs 8",
    "build:fast": "./scripts/optimized-build.sh --skip-deps --no-optimization",
    "export": "echo 'Static export is automatically generated during build'",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "clean": "rm -rf .next out node_modules/.cache .build-cache .temp",
    "config:generate": "node config/environments.js",
    "config:dev": "node config/environments.js development sites-config.development.yaml",
    "config:staging": "node config/environments.js staging sites-config.staging.yaml", 
    "config:prod": "node config/environments.js production sites-config.production.yaml",
    "validate": "node scripts/validate-config-enhanced.js",
    "validate:original": "node scripts/verify-config.js",
    "deploy:walrus": "pnpm run build:export",
    "deploy:walrus:testnet": "./scripts/deploy-walrus-site.sh --network testnet",
    "deploy:walrus:mainnet": "./scripts/deploy-walrus-site.sh --network mainnet",
    "deploy:walrus:force": "./scripts/deploy-walrus-site.sh --force",
    "deploy:dev": "./scripts/deploy-walrus-site.sh --network testnet --site-name waltodo-dev",
    "deploy:staging": "./scripts/deploy-walrus-site.sh --network testnet --site-name waltodo-staging",
    "deploy:prod": "./scripts/deploy-walrus-site.sh --network mainnet --site-name waltodo-prod --force",
    "setup:walrus": "./scripts/setup-walrus-site.sh",
    "setup:walrus:mainnet": "./scripts/setup-walrus-site.sh --network mainnet",
    "analyze": "ANALYZE=true pnpm run build",
    "analyze:bundle": "npx @next/bundle-analyzer",
    "perf:lighthouse": "lighthouse http://localhost:3000 --output html --output-path ./reports/lighthouse.html",
    "test:config": "pnpm run validate && pnpm run config:generate",
    "prebuild": "pnpm run clean",
    "postbuild": "pnpm run validate"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=10.11.0"
  },
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@kurkle/color": "^0.3.2",
    "@mysten/bcs": "^1.1.0",
    "@mysten/dapp-kit": "^0.16.3",
    "@mysten/sui": "^1.30.1",
    "@mysten/wallet-standard": "^0.15.2",
    "@mysten/walrus": "^0.1.1",
    "@tanstack/react-query": "^5.77.2",
    "@tanstack/react-query-devtools": "^5.77.2",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.9",
    "@tanstack/table-core": "^8.21.3",
    "@tanstack/virtual-core": "^3.13.9",
    "@wallet-standard/features": "^1.1.0",
    "@waltodo/shared-types": "workspace:*",
    "chart.js": "^4.4.9",
    "d3-array": "^3.2.4",
    "d3-color": "^3.1.0",
    "d3-format": "^3.1.0",
    "d3-interpolate": "^3.0.1",
    "d3-path": "^3.1.0",
    "d3-scale": "^4.0.2",
