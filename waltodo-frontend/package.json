{
  "name": "@walrus-todo/frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm run setup-config && node scripts/start-with-available-port.js dev",
    "dev:simple": "pnpm run setup-config && next dev",
    "dev:proxy": "pnpm run setup-config && NEXT_PUBLIC_API_URL= next dev",
    "dev:api": "cd ../api && pnpm dev",
    "dev:all": "pnpm run setup-config && concurrently \"pnpm run dev:api\" \"pnpm run dev:proxy\"",
    "build": "pnpm run setup-config && pnpm run typecheck && pnpm run lint && next build",
    "build:dev": "pnpm run setup-config && next build",
    "build:static": "pnpm run setup-config && pnpm run typecheck && pnpm run lint && node scripts/embed-configs.js && NEXT_EXPORT=true next build && node scripts/embed-configs.js restore",
    "build:production": "pnpm run setup-config && pnpm run typecheck && pnpm run lint && NODE_ENV=production next build && node scripts/deploy.js",
    "start": "node scripts/start-with-available-port.js start",
    "start:fixed-port": "next start",
    "dev:fixed-port": "pnpm run setup-config && next dev",
    "lint": "next lint --max-warnings=0",
    "lint:fix": "next lint --fix --max-warnings=0",
    "lint:strict": "eslint . --ext .ts,.tsx --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "typecheck:strict": "tsc --noEmit --strict",
    "fix-dev": "sh ./fix-dev-server.sh",
    "clean": "rm -rf .next node_modules/.cache",
    "dev:clean": "pnpm run clean && node scripts/start-with-available-port.js dev",
    "setup-config": "node setup-config.js",
    "convert-icons": "node scripts/convert-icons.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze:bundle": "node scripts/bundle-analyzer.js analyze",
    "analyze:bundle:report": "node scripts/bundle-analyzer.js report",
    "test:lighthouse": "node scripts/lighthouse-performance.js",
    "test:lighthouse:single": "node scripts/lighthouse-performance.js http://localhost:${PORT:-3000} single",
    "perf:test": "pnpm run analyze:bundle && pnpm run test:lighthouse",
    "build:analyze": "pnpm run build && pnpm run analyze:bundle:report",
    "validate:build": "cd walrus-site-waltodo && ./scripts/build-validator.sh",
    "optimize:assets": "cd walrus-site-waltodo && ./scripts/asset-optimizer.sh",
    "embed-configs": "node scripts/embed-configs.js",
    "restore-configs": "node scripts/embed-configs.js restore",
    "deploy:local": "bash scripts/deploy-local.sh",
    "deploy:local:prod": "bash scripts/deploy-local.sh prod",
    "deploy:local:static": "bash scripts/deploy-local.sh static",
    "deploy:production": "bash scripts/deploy-production.sh",
    "deploy:walrus": "bash scripts/deploy-walrus-sites.sh",
    "deploy:walrus:testnet": "bash scripts/deploy-walrus-sites.sh testnet",
    "deploy:walrus:mainnet": "bash scripts/deploy-walrus-sites.sh mainnet",
    "health:check": "bash scripts/deploy-local.sh check",
    "info": "bash scripts/deploy-local.sh info",
    "recover:diagnose": "cd walrus-site-waltodo && ./scripts/build-recovery.sh diagnose",
    "recover:clean": "cd walrus-site-waltodo && ./scripts/build-recovery.sh clean --auto",
    "recover:rebuild": "cd walrus-site-waltodo && ./scripts/build-recovery.sh rebuild --auto",
    "recover:emergency": "cd walrus-site-waltodo && ./scripts/build-recovery.sh emergency --auto"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=10.11.0"
  },
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
    "@mysten/bcs": "^1.1.0",
    "@mysten/dapp-kit": "^0.16.3",
    "@mysten/sui": "^1.30.1",
    "@mysten/wallet-standard": "^0.15.2",
    "@mysten/walrus": "^0.1.1",
    "@tanstack/react-query": "^5.77.2",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.9",
    "@tanstack/table-core": "^8.21.3",
    "@tanstack/virtual-core": "^3.13.9",
    "@wallet-standard/features": "^1.1.0",
    "@waltodo/shared-types": "workspace:*",
    "d3-array": "^3.2.4",
    "d3-color": "^3.1.0",
    "d3-format": "^3.1.0",
    "d3-interpolate": "^3.0.1",
    "d3-path": "^3.1.0",
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.2.0",
    "d3-time": "^3.1.0",
    "d3-time-format": "^4.1.0",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.6",
    "encoding": "^0.1.13",
    "framer-motion": "^12.15.0",
    "fuse.js": "^7.1.0",
    "goober": "^2.1.16",
    "idb": "^8.0.3",
    "immer": "^10.1.1",
    "internmap": "^2.0.3",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.511.0",
    "nanoid": "^5.1.5",
    "next": "^15.3.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hot-toast": "^2.5.2",
    "react-swipeable": "^7.0.2",
    "react-transition-group": "^4.4.5",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "recharts": "^2.15.3",
    "recharts-scale": "^0.4.5",
    "sharp": "^0.34.2",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.0",
    "tiny-invariant": "^1.3.3",
    "web-vitals": "^5.0.2",
    "zod": "^3.25.46",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@tanstack/react-query-devtools": "^5.79.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "22.0.0",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "@types/react-window": "^1.8.8",
    "@types/react-window-infinite-loader": "^1.0.9",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "assert": "^2.1.0",
    "autoprefixer": "10.4.20",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "chrome-launcher": "^1.1.0",
    "compression": "^1.7.4",
    "concurrently": "^8.2.2",
    "constants-browserify": "^1.0.0",
    "crypto-browserify": "^3.12.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "15.3.2",
    "events": "^3.3.0",
    "express-rate-limit": "^7.4.1",
    "get-port": "7.1.0",
    "helmet": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^12.0.0",
    "pino-pretty": "^13.0.0",
    "postcss": "8.5.4",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "sinon": "^19.0.0",
    "stream-browserify": "^3.0.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "3.4.0",
    "typescript": "5.8.3",
    "url": "^0.11.4",
    "util": "^0.12.5"
  }
}
